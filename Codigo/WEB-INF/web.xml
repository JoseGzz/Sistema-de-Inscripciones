<?xml version="1.0" encoding="ISO-8859-1" ?> 
<web-app xmlns="http://java.sun.com/xml/ns/javaee"  
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee  
           http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"  
         version="2.5">

    <servlet>
      <servlet-name>Registro</servlet-name>
      <servlet-class>interfaces.InterfazRegistroTaller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>RegistroBeneficiario</servlet-name>
      <servlet-class>interfaces.InterfazRegistroBeneficiario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistroBeneficiario</servlet-name>
        <url-pattern>/RegistroBeneficiario</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>PerfilBeneficiario</servlet-name>
      <servlet-class>interfaces.InterfazPerfilBeneficiario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PerfilBeneficiario</servlet-name>
        <url-pattern>/PerfilBeneficiario</url-pattern>
    </servlet-mapping>


    <servlet>
      <servlet-name>InterfazInfoTaller</servlet-name>
      <servlet-class>interfaces.InterfazInfoTaller</servlet-class>
    </servlet>
    <servlet-mapping>
